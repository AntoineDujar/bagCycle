---
import Flick from "@components/Flick.svelte";
import ImageWrapper from "@components/ImageWrapper.astro";
---

<Flick client:idle>
  <img
    slot="image-1"
    class="ca"
    src="./assets/bag1.jpg"
    alt="image"
    height="800"
  />
  <img
    slot="image-2"
    class="ca"
    src="./assets/bag2.jpg"
    alt="image"
    height="800"
  />
  <img
    slot="image-3"
    class="ca"
    src="./assets/bag3.jpg"
    alt="image"
    height="800"
  />
  <img
    slot="image-4"
    class="ca"
    src="./assets/bag4.jpg"
    alt="image"
    height="800"
  />
</Flick>

<style>
  .ca {
    width: 30rem;
    margin-bottom: 0rem;
    border-radius: 8px;
  }
</style>
